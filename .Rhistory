require(forecast)
library(forecast)
for (i in 1:10){
ts.now <- ts(ItemLast[i], start=c(2013, 1), end=c(2015, 12), frequency=12)
#plot(ts.now)
accuracyarray = c()
#hangi aydan başlıcaz
#ts.now=ts.now [1:(length(ts.now)-3)]
#ts.nowt <- ts.now#okun yönünü değiştirdim
#ts.nowt=ts.nowt [1:(length(ts.now)-3)] ##2 aylık mı bakcaz
for (j in c(2:4)){
ts.now->ts.nowt
ts.nowt=ts(ts.nowt[1:(length(ts.now)-j)],
start=start(ts.now), frequency=frequency(ts.now))
fit <- auto.arima(ts.nowt)
arima=forecast(fit,h=j)
accuracyarima=accuracy ( arima,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
seso=ses(ts.nowt,h=j,level=c(80,95),fan=FALSE,initial=c("optimal"),alpha=NULL)
accuracyseso=accuracy ( seso,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
sess=ses(ts.nowt,h=j,level=c(80,95),fan=FALSE,initial=c("simple"),alpha=NULL)
accuracysess=accuracy ( sess,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
cr=croston(ts.nowt,h=j,alpha=0.1)
accuracycr=accuracy ( cr,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
f=forecast(ts.nowt,h=j, level=c(1,5), fan=FALSE, robust=FALSE, lambda=NULL,find.frequency=FALSE,restrict=FALSE, allow.multiplicative.trend=TRUE)
accuracyf=accuracy ( f,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
mf=meanf(ts.nowt, h=j, fan=FALSE, lambda=NULL, level=c(1,5))
accuracymf=accuracy ( mf,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
rwf=rwf(ts.nowt, h=j, drift=TRUE, level=c(80,95), fan=FALSE, lambda=NULL)
accuracyrwf=accuracy ( mf,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
#m.avg=ma(ts.nowt, order=2)
#accuracymavg=accuracy ( m.avg,ts.now[(length(ts.now)-6):length(ts.now)], test = NULL, d = NULL, D = NULL)
holt=holt(ts.nowt, h=j, level=c(80,95), fan=FALSE,initial=c("optimal"), exponential=FALSE, alpha=NULL, beta=NULL)
accuracyholt=accuracy ( holt,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
holt2=holt(ts.nowt, h=j, level=c(80,95), fan=FALSE,initial=c("simple"), exponential=FALSE, alpha=NULL, beta=NULL)
accuracyholt2=accuracy ( holt2,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
#hw=hw(ts(ts.nowt,frequency=12), h=j, seasonal=c("additive"), level=c(80,95),fan=FALSE,initial=c("optimal"), exponential=FALSE, alpha=NULL, beta=NULL, gamma=NULL)
#accuracyhw=accuracy ( hw,ts.now[(length(ts.now)-3):length(ts.now)], test = NULL, d = NULL, D = NULL)
hw.result = try(hw(ts(ts.nowt,frequency=12), h=j, seasonal=c("additive"),
level=c(80,95),fan=FALSE,initial=c("optimal"),
exponential=FALSE, alpha=NULL, beta=NULL, gamma=NULL),
silent=FALSE)
if (!inherits(hw.result, "try-error")){
accuracyhw=accuracy (hw.result,
ts.now[(length(ts.now)-(j-1)):length(ts.now)],
test = NULL, d = NULL, D = NULL)
} else {
accuracyhw = Inf
}
spl=splinef(ts.nowt, h=j, level=c(80,95), fan=FALSE, lambda=NULL, method=c("mle"))
accuracyspl=accuracy ( spl,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
theta=thetaf(ts.nowt, h=j, level=c(80,95), fan=FALSE)
accuracytheta=accuracy ( theta,ts.now[(length(ts.now)-(j-1)):length(ts.now)], test = NULL, d = NULL, D = NULL)
#accuracyarray=c(accuracyarima[11], accuracyseso[11], accuracysess[11], accuracycr[11], accuracyf[11], accuracymf[11], accuracyrwf[11], accuracymavg[11], accuracyholt[11], accuracyholt2[11], accuracyhw[11], accuracyspl[11], accuracytheta[11])
accuracyar <- c(accuracyarima["Test set","MAE"], accuracyseso["Test set","MAE"],
accuracysess["Test set","MAE"],accuracycr["Test set","MAE"],
accuracyf["Test set","MAE"],
accuracymf["Test set","MAE"], accuracyrwf["Test set","MAE"],
accuracyholt["Test set","MAE"],
accuracyholt2["Test set","MAE"], accuracyhw["Test set","MAE"],
accuracyspl["Test set","MAE"], accuracytheta["Test set","MAE"])
accuracyarRMSE <- c(accuracyarima["Test set","RMSE"], accuracyseso["Test set","RMSE"],
accuracysess["Test set","RMSE"], accuracyf["Test set","RMSE"],
accuracymf["Test set","RMSE"], accuracyrwf["Test set","RMSE"],
accuracyholt["Test set","RMSE"],accuracycr["Test set","RMSE"],accuracymavg["Test set","RMSE"],
accuracyholt2["Test set","RMSE"], accuracyhw["Test set","RMSE"],
accuracyspl["Test set","RMSE"], accuracytheta["Test set","RMSE"])
selected=which.min(accuracyar)
accuracyarray = rbind(accuracyarray,
data.frame(selected=selected, j=j,
accuracy=accuracyar[selected]))
} ## for j loop
if ( selected==1){
fit <- auto.arima(ts.now)
arima=forecast(fit,h=12)
plot(arima,type="o", lwd="1")
frcst= arima
}else if (selected==2){
seso=ses(ts.now,h=12,level=c(80,95),fan=FALSE,initial=c("optimal"),alpha=NULL)
plot(seso, ylab="Demand",
xlab="Year",type="o")
frcst=seso
}else if (selected==3){
sess=ses(ts.now,h=12,level=c(80,95),fan=FALSE,initial=c("simple"),alpha=NULL)
plot(sess, ylab="demand",
xlab="Year",type="o")
frcst= sess
}else if (selected==4){
cr<-croston(ts.now,h=12,alpha=0.1)
plot(cr)
frcst=cr
}else if (selected==5){
f=forecast(ts.now,h=12, level=c(1,5), fan=FALSE, robust=FALSE, lambda=NULL,find.frequency=FALSE,restrict=FALSE, allow.multiplicative.trend=TRUE)
frcst=f
}else if (selected==6){
mf=meanf(ts.now, h=12, fan=FALSE, lambda=NULL, level=c(1,5))
frcst=mf
plot(mf)
}else if (selected==7){
rwf=rwf(ts.now, h=12, drift=TRUE, level=c(80,95), fan=FALSE, lambda=NULL)
frcst=rwf
plot(rwf)
}else if (selected==8){
holt=holt(ts.now, h=12, level=c(80,95), fan=FALSE,initial=c("optimal"), exponential=FALSE, alpha=NULL, beta=NULL)
frcst=holt
plot(holt)
}else if (selected==9){
holt2=holt(ts.now, h=12, level=c(80,95), fan=FALSE,initial=c("simple"), exponential=FALSE, alpha=NULL, beta=NULL)
frcst=holt2
}else if (selected==10){
hw=hw(ts(ts.now,frequency=12), h=12, seasonal=c("additive"), level=c(80,95),fan=FALSE,initial=c("optimal"), exponential=FALSE, alpha=NULL, beta=NULL, gamma=NULL)
plot(hw, type="o", lwd="1")
frcst=hw
}else if (selected==11){
spl=splinef(ts.now, h=12, level=c(80,95), fan=FALSE, lambda=NULL, method=c("mle"))
frcst=spl
plot(spl)
}else{
theta=thetaf(ts.now, h=12, level=c(80,95), fan=FALSE)
plot(theta)
frcst=theta
}
}
selected
